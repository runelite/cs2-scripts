// 7913
[proc,script7913]
if (%settings_sd_beta_enabled = 0) {
	mes("New renderer beta is not active, you can't toggle it now!");
	return;
}
if (%settings_hd_new_renderer_toggle = 1) {
	rt7_sd;
	rt7_setenabled(true);
	mes("New renderer enabled!");
} else {
	rt7_setenabled(false);
	mes("New renderer disabled!");
}
~script7915;
