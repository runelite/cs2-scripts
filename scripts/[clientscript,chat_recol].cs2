// 2065
[clientscript,chat_recol](component $layer0, int $int1, int $colour2)
if (cc_find($layer0, calc($int1 * 4)) = ^true) {
	cc_setcolour($colour2);
}
if (cc_find($layer0, calc($int1 * 4 + 1)) = ^true) {
	cc_setcolour($colour2);
}
if (cc_find($layer0, calc($int1 * 4 + 2)) = ^true) {
	cc_setcolour($colour2);
}
