// 290
[proc,bankmain_itemnote](component $component0)
if (%bank_withdrawnotes = 1) {
	if_setgraphic("miscgraphics,9", $component0);
	if_setonop("bankmain_itemnote_op(event_op, $component0, 0)", $component0);
	if_setop(1, "Disable <col=ff9040>Notes", $component0);
} else {
	if_setgraphic("miscgraphics,0", $component0);
	if_setonop("bankmain_itemnote_op(event_op, $component0, 1)", $component0);
	if_setop(1, "Enable <col=ff9040>Notes", $component0);
}
